{
	"manifest_version": 2.0,
	"id": "graphprototype",
	"type": "widget",
	"name": "Graph prototype",
	"namespace": "GraphPrototype",
	"version": "1.0",
	"author": "Zabbix",
	"description": "Displays a grid of graphs created by low-level discovery from either a graph prototype or an item prototype.",
	"url": "https://www.zabbix.com/documentation/7.0/en/manual/web_interface/frontend_sections/dashboards/widgets/graph_prototype",
	"widget": {
		"size": {
			"width": 16,
			"height": 5
		},
		"js_class": "CWidgetGraphPrototype",
		"in": {
			"time_period": {
				"type": "_timeperiod"
			}
		},
		"out": [
			{
				"type": "_timeperiod"
			}
		]
	},
	"actions": {
		"widget.graphprototype.view": {
			"class": "WidgetView",
			"layout": "layout.json",
			"view": null
		}
	},
	"assets": {
		"js": ["class.widget.js"]
	}
}
