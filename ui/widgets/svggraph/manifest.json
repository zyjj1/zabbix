{
	"manifest_version": 2.0,
	"id": "svggraph",
	"type": "widget",
	"name": "Graph",
	"namespace": "SvgGraph",
	"version": "1.0",
	"author": "Zabbix",
	"description": "Displays data of up to 50 items as line, points, staircase, or bar charts.",
	"url": "https://www.zabbix.com/documentation/7.0/en/manual/web_interface/frontend_sections/dashboards/widgets/graph",
	"widget": {
		"js_class": "CWidgetSvgGraph",
		"in": {
			"time_period": {
				"type": "_timeperiod"
			}
		},
		"out": [
			{
				"type": "_timeperiod"
			}
		]
	},
	"actions": {
		"widget.svggraph.view": {
			"class": "WidgetView"
		}
	},
	"assets": {
		"js": ["class.widget.js"]
	}
}
